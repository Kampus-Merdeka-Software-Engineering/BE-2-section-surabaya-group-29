datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model News {
    id Int @id @default(autoincrement())
    createdAt DateTime @default(now())
    judul String 
    deskripsi String 
    isiBerita String
    penulis String 
    foto String 
}
